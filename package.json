{
  "name": "sls-publish-bot",
  "version": "1.3.0",
  "description": "Publish telegram bot for freedom blogs",
  "main": "src/index.ts",
  "type": "module",
  "repository": "https://github.com/bozonx/sls-publish-bot.git",
  "author": "Kozyrin Ivan <ipkozyrin@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "ts-node-esm ./src/index.ts",
    "prod": "yarn ts-node --esm ./src/index.ts",
    "container": "mkdir -p ${DATA_DIR} && yarn install --prod && yarn prod",
    "typecheck": "tsc --noEmit ./src/index.ts",
    "build": "_useless/docker build -f ./docker/Dockerfile -t bozonx/publish_bot:latest .",
    "push": "_useless/docker push bozonx/publish_bot:latest",
    "prod-test-run": "_useless/docker run --name sls-bot -ti --rm bozonx/publish_bot:latest"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.3",
    "@types/dotenv": "^8.2.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.191",
    "@types/mdast": "^3.0.10",
    "@types/node": "^18.14.0",
    "better-telegraph": "^2.1.1",
    "dotenv": "^16.0.3",
    "hast-util-from-html": "^1.0.1",
    "hast-util-to-html": "^8.0.4",
    "hast-util-to-mdast": "^9.0.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mdast-util-from-markdown": "^1.3.0",
    "mdast-util-to-markdown": "^1.5.0",
    "moment": "^2.29.4",
    "telegraf": "^4.11.2",
    "telegraf-entity": "^1.1.0",
    "telegram-format": "^2.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "googleapis": "^111.0.0"
  }
}
